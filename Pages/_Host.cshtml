}

<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Название сайта</title>
    <base href="~/" />
    <link id="my" href="../bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="../font-awesome/css/all.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css2?family=Source+Sans+Pro:wght@300;400;600;900&display=swap" rel="stylesheet">
</head>
<body class="bg-light text-dark">
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <script src="~/jquery/jquery.slim.min.js"></script>
    <script src="~/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/font-awesome/js/all.min.js"></script>
    <script src="_framework/blazor.server.js"></script>

    <script src="~/my/ChangeTheme.js"></script>
